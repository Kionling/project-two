<h3>Hiking Log</h3>

<ul class="collection">
    {{#each entries}}
    <li class="collection-item">
        <h4 style="font-weight: 900;">{{dataValues.title}}</h4>
        <h5 style="font-weight: 600;" >{{dataValues.dateHiked}}</h5>
        <p>{{dataValues.description}}</p>
    </li>
    {{/each}}

    {{!-- {{#each trails}}
    <iframe src={{mapUrl}}></iframe>
    {{/each}} --}}
</ul>